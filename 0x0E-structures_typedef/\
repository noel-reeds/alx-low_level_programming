#include <stdio.h>
#include "dog.h"
/**
  * print_dog - prints a struct dog.
  *
  *@d:
  *
  *Return:
  */
void print_dog(struct dog *d)
{
	if (d == NULL)
	{
		NULL;
	}
	else if (*name == NULL)
	{
		printf("Name: (nil)\n");
		printf("Age: %f\n", d->age);
		printf("Owner: %s\n", d->owner);
	}
	else if (*owner == NULL)
	{
		printf("Name: %s\n", d->name);
		printf("Age: %f\n", d->age);
		printf("Owner: (nil)\n");
	}
	printf("Name: %s\n", d->name);
	printf("Age: %f\n", d->age);
	printf("Owner: %s\n", d->owner);
}
